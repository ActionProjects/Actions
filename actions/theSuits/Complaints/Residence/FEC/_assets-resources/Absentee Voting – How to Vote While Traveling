.um-folding-cube .um-cube2 { transform: scale(1.1) rotateZ(90deg); }

.um-folding-cube .um-cube3 { transform: scale(1.1) rotateZ(180deg); }

.um-folding-cube .um-cube4 { transform: scale(1.1) rotateZ(270deg); }

