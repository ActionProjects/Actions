@media only screen and (min-width: 758px) {
  .hfsh .search-icon { display: inline-block; margin: 0px 0px 0px auto; background: transparent; color: inherit; opacity: 0.6; cursor: pointer; padding: 0px 12px; outline: none; }
}

.hfsh .search-icon:hover { opacity: 1; }

.hfsh .search-icon:active { opacity: 0.5; }

