@media not all {
  .hfsh .menu .menu-section-main .menu-list { flex-flow: row wrap; }
}

.hfsh .menu .menu-section-main .menu-list .menu-list-item { width: 50%; margin-bottom: 15px; }

@media only screen and (min-width: 758px) {
  .hfsh .menu .menu-section-main .menu-list .menu-list-item { width: 33%; }
}

@media only screen and (min-width: 1000px) {
  .hfsh .menu .menu-section-main .menu-list .menu-list-item { width: 50%; }
}

